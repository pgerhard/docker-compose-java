<settings>
    <servers>
        <server>
            <id>ossrh</id>
            <username>${env.OSSRH_TOKEN_USERNAME}</username>
            <password>${env.OSSRH_TOKEN_PASSWORD}</password>
        </server>
        <server>
            <id>ossrh-snapshots</id>
            <username>${env.OSSRH_TOKEN_USERNAME}</username>
            <password>${env.OSSRH_TOKEN_PASSWORD}</password>
        </server>
    </servers>
    <profiles>
        <profile>
            <id>ci</id>
            <properties>
                <gpg.executable>gpg</gpg.executable>
                <gpg.keyname>${env.GPG_KEY}</gpg.keyname>
                <gpg.passphrase>${env.GPG_PWD}</gpg.passphrase>
            </properties>
        </profile>
    </profiles>
</settings>